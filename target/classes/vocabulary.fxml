                <TextField fx:id="meaningTextField" prefHeight="31.0" prefWidth="269.0" promptText="add your meaning here" GridPane.rowIndex="7" />
                <Button fx:id="addButton" mnemonicParsing="false" onAction="#handle_AddButton" prefHeight="31.0" prefWidth="269.0" text="Add" GridPane.rowIndex="9">
                    <font>
                        <Font name="System Bold" size="15.0" />
                    </font>
                </Button>
            </children>
        </GridPane>

        <TableView fx:id="dictionaryTableView" layoutX="291.0" layoutY="48.0" prefHeight="341.0" prefWidth="520.0">
            <columns>
                <TableColumn prefWidth="49.0" text="No.">
                    <cellValueFactory>
                        <PropertyValueFactory property="id" />
                    </cellValueFactory>
                </TableColumn>

                <TableColumn minWidth="0.0" prefWidth="134.0" text="Word">
                    <cellValueFactory>
                        <PropertyValueFactory property="word" />
                    </cellValueFactory>
                </TableColumn>

