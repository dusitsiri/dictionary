            <children>
                <Label prefHeight="21.0" prefWidth="95.0" text="Word" />
                <TextField fx:id="wordTextField" prefHeight="31.0" prefWidth="269.0" promptText="add your word here" GridPane.rowIndex="1" />
                <Label prefHeight="17.0" prefWidth="100.0" text="Type" GridPane.rowIndex="3" />
            <ComboBox fx:id="typeWordBox" prefWidth="150.0" promptText="select type" GridPane.rowIndex="4" />
